{
  "name": "gatsby-plugin-anchor-links",
  "version": "1.1.1",
  "main": "lib",
  "author": "Chase Ohlson <chase@chaseohlson.com>",
  "description": "Gatsby plugin for using anchor links with a Gatsby Link component.",
  "homepage": "https://github.com/brohlson/gatsby-plugin-anchor-links",
  "license": "MIT",
  "scripts": {
    "build": "babel lib/ --out-dir . --ignore __tests__",
    "format": "prettier --write \"**/*.{js,jsx,ts,json,md}\"",
    "watch": "babel -w lib/ --out-dir . --ignore __tests__"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.4",
    "@babel/core": "^7.10.4",
    "@babel/helper-compilation-targets": "^7.10.4",
    "@babel/preset-env": "^7.10.4",
    "@babel/preset-react": "^7.10.4",
    "gatsby": "^2.19.10",
    "jest": "^25.1.0",
    "prettier": "^1.19.1",
    "react": "^16.12.0"
  },
  "keywords": [
    "gatsby",
    "gatsby-plugin",
    "gatsby-source-plugin",
    "gatsby-anchor-links",
    "gatsby-plugin-anchor-links"
  ],
  "dependencies": {
    "scroll-to-element": "^2.0.3"
  }
